{
  "name": "copy-title",
  "description": "Build an Extension!",
  "version": "2.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.jpg"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "激活该扩展程序"
    }
  },
  "permissions": ["activeTab", "clipboardRead", "clipboardWrite"]
}
